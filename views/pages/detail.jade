extends ../layout
block content
	.container
		.row
			.col-md-7
				embed(src="#{movie.flash}",allowFullScreen='true',quality="high",width="720",height="600",align="middle",type="application/x-shockwave-flash")
			.col-md-4.col-md-offset-1
				dl.dl-horizontal
				dt 电影名字
				dt #{movie.title}
				dt 电影 director
				dt #{movie.director}
				dt country
				dt #{movie.country}
				dt language
				dt #{movie.language}
				dt introduce
				dt #{movie.desc}

